<h1>Listing developers</h1>

<%= link_to 'New Developer', new_developer_path %>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @developers.each do |developer| %>
    <tr>
      <td><%= developer.first_name %></td>
      <td><%= developer.last_name %></td>
      <td><%= link_to 'Destroy', developer, method: :delete %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

